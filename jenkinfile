@Library('dhinesh') _

import com.cleverbuilder.GlobalVars
import com.cleverbuilder.SampleClass
pipeline
{
agent{
label 'slave1'
}
stages
{
stage("git stage"){
steps{
git 'https://github.com/dhinesh0307/dhineshfirstio.git'
}
}
stage("docker build"){

steps{
 script {
                    def person = new SampleClass()
                    person.age = 21
                    person.increaseAge(10)
                    echo 'Incremented age, is now : ' + person.age
                }
}
}
}
}
